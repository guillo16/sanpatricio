<div class="main-card-article">
  <%= link_to article_path(article) do %>
  <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= article.photo %>)">
  <h4><%= article.category %></h4>
  <h2><%= article.title %></h2>
  </div>
  <% end %>
</div>
