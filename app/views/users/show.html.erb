<div class="user-wrapper">
  <h1>BIENVENIDO</h1>
  <h3>Usuario </h3>
</div>


<%= @user.email %>
<%= link_to 'crear noticia', new_article_path  %>
<%= link_to 'crear evento', new_article_path  %>




<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Usuario</th>
      <th scope="col">Creado</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each_with_index do | user, index| %>
    <% users = index + 1 %>
    <tr>
      <th scope="row"><%= users %></th>
      <td><%= user.email %></td>
      <td><%= user.created_at.strftime('%y/%m/%Y a las %H:%m') %></td>
    </tr>
    <% end %>
  </tbody>
</table>
