<%= render 'shared/top' %>
<%= render 'shared/navbar' %>
<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.3),rgba(0,0,0,0.3)), url(https://res.cloudinary.com/dw7ox75dg/image/upload/c_scale,w_1317/v1589459748/San%20patricio/IMG_3902-01.jpg);">
  <div class="banner-container">
    <div class="banner-image">
      <img src="https://res.cloudinary.com/dw7ox75dg/image/upload/v1588966208/loguito.png" alt="">
      <img src="https://res.cloudinary.com/dw7ox75dg/image/upload/v1584881920/ib-world-school-logo-1-colour-rev.png" alt="" style="height: 150px;">
    </div>
    <p>
        <span id="banner-typed-text-secondary"></span>
      </p>
  </div>
</div>
<section id="articles">
  <%= render 'shared/articles' %>
</section>
<section id="ib">
  <%= render 'shared/ib' %>
</section>
<section id="events">
  <div class="home-container">
    <h3>CALENDARIO</h3>
    <h5>Los proximos eventos del colegio <i class="fas fa-caret-down"></i></h5>
    <div class="event-container-home">
      <% @events.each do |event| %>
      <%= render 'cards/event_main_cards', event: event %>
      <% end %>
    </div>
    <div class="article-button">
      <%= link_to 'EVENTOS', events_path, class: "btn-article"  %>
    </div>
  </div>
</section>
<section id="social">
  <%= render 'shared/social' %>
</section>
<section>
  <div class="text-center">
    <p style="color: #F4F4F4;">p</p>
  </div>
</section>
