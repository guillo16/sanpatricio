<div id="carouselNiveles" class="carousel slide" data-ride="carousel">
  <ol class="carousel-indicators">
    <li data-target="#carouselNiveles" data-slide-to="0" class="active"></li>
    <li data-target="#carouselNiveles" data-slide-to="1"></li>
    <li data-target="#carouselNiveles" data-slide-to="2"></li>
